{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data":{"url": "ne_110m_admin_0_countries.json",
            "format": {"type": "topojson"}},
    "mark": "geoshape",
    "params": [
        {
        "name": "year",
        "value": 2018,
        "bind": {
            "input": "range",
            "min": 1990,
            "max": 2019,
            "step": 1,
            "name": "Year: "
        }
    }],        
    "projection": {"type": "equalEarth"},
    "transform": [
        {
        "lookup": "properties.NAME",
        "from": {
            "data": {
                "url": "datavis2.csv"},
            "key": "Entity",
            "fields": ["Active"]
        }
    }
    ],
    "encoding":{
        "color":{"field": ""}
    }
}