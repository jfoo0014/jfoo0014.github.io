{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data":{"url": "https://github.com/jfoo0014/jfoo0014.github.io/blob/0a6fc6b57eb34cf33fc562181841d5f0df564322/ne_110m_admin_0_countries.json",
            "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}},
    "params": [
        {
        "name": "year",
        "value": 2018,
        "bind": {
            "input": "range",
            "min": 1990,
            "max": 2019,
            "step": 1,
            "name": "Year: "
        }
    }],         
    "projection": {"type": "equalEarth"},
    "transform": [
        {
        "lookup": "properties.ADMIN",
        "from": {
            "data": {
                "url": "datavis2.csv"},
            "key": "Entity",
            "fields": ["Internet Usage"]
        }
    }
    ],
    "mark": {"type":"geoshape"},
    "encoding":{
        "color":{
            "field": "Internet Usage",
            "type": "quantitative"
        }
    }
}