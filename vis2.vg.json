{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width":500,
    "height":500,
    "data":{"url": "https://raw.githubusercontent.com/jfoo0014/jfoo0014.github.io/main/ne_110m_admin_0_countries.topojson",
            "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}},
    "params": [
        {
        "name": "year",
        "value": 1990,
        "bind": {
            "input": "range",
            "min": 1990,
            "max": 2019,
            "step": 1,
            "name": "Year: "
        }
    }],         
    "projection": {"type": "equalEarth"},
    "transform": [
        {
        "lookup": "properties.ADMIN",
        "from": {
            "data": {
                "url": "https://raw.githubusercontent.com/jfoo0014/jfoo0014.github.io/main/datavis2.csv"},
            "key": "Entity",
            "fields": ["Internet Usage"]
        }
    }],
    "mark": {"type":"geoshape"},
    "encoding":{
        "color":{
            "field": "Internet Usage",
            "type": "quantitative"
        },
        "tooltip": [{"field": "properties.ADMIN", "title": "Country"},{"field": "Internet Usage"}]
    }
}