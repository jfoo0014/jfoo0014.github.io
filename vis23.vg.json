{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width":600,
    "height":300,
    "title":"",       
    "projection": {"type": "equalEarth"},
    "data": {
        "url": "https://raw.githubusercontent.com/jfoo0014/jfoo0014.github.io/main/datavis2.csv"},
    "transform": [
        {"calculate": "datum.LifeExpectancy", "as": "Life Expectancy per Internet"}
    ],
    "mark": {"type":"point"},
    "encoding":{
        "x":{"field": "InternetUsage", "type": "quantitative"},
        "y":{"field": "LifeExpectancy", "type": "quantitative"},
        "color":{
            "field": "LifeExpectancy",
            "type": "quantitative",
            "title": "Life Expectancy at Birth"
        },
        "tooltip": [{"field": "Entity", "title": "Country"},{"field": "LifeExpectancy"}]
    }
}